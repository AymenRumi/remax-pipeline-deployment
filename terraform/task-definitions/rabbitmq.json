[
    {
      "name": "rabbitmq",
      "image": "rabbitmq:3-management",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5672,
          "hostPort": 5672
        },
        {
          "containerPort": 15672,
          "hostPort": 15672
        }
      ],
      "environment": [
        {
          "name": "RABBITMQ_DEFAULT_USER",
          "value": "user"
        },
        {
          "name": "RABBITMQ_DEFAULT_PASS",
          "value": "password"
        }
      ]
    }
]